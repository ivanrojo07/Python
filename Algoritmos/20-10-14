#En cuantos periodos alcanzara los 100 casos
def numCasos(base,numPeriodos):
    resultado = base
    for c in range (numPeriodos):
        resultado= resultado *1.3742
    return int (resultado)

def periodosCasos(casos):
    numPeriodos=0
    for c in numPeriodos:
        if numCasos(72, numPeriodos) <= casos:
            numPeriodos = numPeriodos + 1
    return numCasos(72,numPeriodos)
