for c in a:
    print c #contador
    for d in b:
        print d #repite esta cadena c veces
for c in a:
    print c
for d in b:
        print d
print ("M" in b)# la funcion "m" in b te dice si hay ese elemento en el arreglo.
#Hacer una funcion

def esPar (num):#def para definir una funcion
    if(num %2) ==0:
        return True
    else:
        return False
c in range (10)
print (c, esPar(c))
