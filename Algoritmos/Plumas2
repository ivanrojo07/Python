def paqPlumas(num):
    if num in [0,1,2,3,4,6,7,9,11,12,14,17,19,22,27]:
        return False
    else:
        return True

def paqPlumas2(num):
    return not (num in [0,1,2,3,4,6,7,9,11,12,14,17,19,22,27])

for c in range(50):
    print c,paqPlumas(c)
    print c,paqPlumas2(c)
for c in range (5):
    x= int(raw_input("Escribe el numero a probar: "))
    print x
    print (x,paqPlumas(x))
    print (x,paqPlumas2(x))
    
